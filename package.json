{
  "name": "property-manager-decorator",
  "version": "2.0.0-alpha.0",
  "description": "ES201X/TypeScript class decorator for property-manager",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "source": "src/index.ts",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npm run build.ts",
    "build.ts": "npm run build.ts.cjs && npm run build.ts.mjs",
    "build.ts.cjs": "tsc -p tsconfig.build.json --module commonjs -outDir lib",
    "build.ts.mjs": "tsc -p tsconfig.build.json --moduleResolution nodenext --module es2022 -outDir lib/esm",
    "clean": "rimraf ./lib",
    "lint": "eslint --ext js,jsx,ts,tsx,vue .",
    "release": "npm run clean && npm run build && npx commit-and-tag-version -s",
    "release.alpha": "npm run release -- --prerelease alpha",
    "test": "jest",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snowyu/property-manager-decorator.ts.git"
  },
  "keywords": [
    "property-manager",
    "attribute",
    "property",
    "properties",
    "manage",
    "manager",
    "decorator",
    "class",
    "serialization",
    "deserialization",
    "typescript"
  ],
  "author": "Riceball LEE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snowyu/property-manager-decorator.ts/issues"
  },
  "homepage": "https://github.com/snowyu/property-manager-decorator.ts#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/memoizee": "^0.4.8",
    "@types/node": "^18.15.11",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lodash": "^4.17.21",
    "memoizee": "*",
    "property-manager": "^2.0.0-alpha.3",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "lodash": ">=2",
    "memoizee": "*",
    "property-manager": ">=2"
  }
}
